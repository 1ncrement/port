nav.navbar.navbar-fixed-top
	.container
		.navbar-header
			button.navbar-toggle.collapsed.btn(type="button", data-toggle="collapse", data-target="#menu", aria-expanded="false")
				span.sr-only Toggle
				span.glyphicon.glyphicon-align-justify
			a(href="#{mainLink}").navbar-brand My Logo
		.collapse.navbar-collapse#menu
			- var social = [{link:'http://vk.com/vakula_e_g',icon:'vk'},{link:'https://www.facebook.com/profile.php?id=100004015285077',icon:'facebook'},{link:'https://www.linkedin.com/in/%D0%B5%D0%B2%D0%B3%D0%B5%D0%BD%D0%B8%D0%B9-%D0%B2%D0%B0%D0%BA%D1%83%D0%BB%D0%B5%D0%BD%D0%BA%D0%BE-bb422910a?trk=nav_responsive_tab_profile',icon:'linkedin'},{link:'https://plus.google.com/u/0/110067773716162418802',icon:'google-plus'}]
			- var menu = [{name:'Main',link:'/'},{name:'Features',link:'#'},{name:'Works',link:'#'},{name:'About me',link:'#'}]
			ul.nav.navbar-nav.navbar-left
				-each val in social
					li
						a(href="#{val.link}", target="_blank")
							span.fa-stack
								i.fa.fa-square-o.fa-stack-2x
								i(class="fa fa-#{val.icon} fa-stack-1x")
			ul.nav.navbar-nav.navbar-right
				- each val in menu
					li
						a(href="#{mainLink + val.link}") #{val.name}